{
  "manifest_version": 3,
  "name": "FloatConsole",
  "version": "1.0.0",
  "description": "A floating console that captures logs, warnings, and errors for quick debugging",
  "action": {
    "default_icon": "data:image/svg+xml;base64,",
    "default_popup": "popup/popup.html",
    "default_title": "Configure FloatConsole"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "commands": {
    "toggle_dock": {
      "description": "Toggle the FloatConsole on/off",
      "suggested_key": {
        "default": "Ctrl+Shift+X",
        "mac": "Command+Shift+X"
      }
    }
  },
  "content_scripts": [{
    "js": ["content/init.js"],
    "matches": ["<all_urls>"],
    "run_at": "document_start"
  }],
  "permissions": ["storage"],
  "web_accessible_resources": [{
    "matches": ["<all_urls>"],
    "resources": ["content/cleanupLogger.js", "content/logger.js"]
  }]
}
